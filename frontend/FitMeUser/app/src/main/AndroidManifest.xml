<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitMe"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Activity.SignInNumber"
            android:exported="false" />
        <activity
            android:name=".Activity.EditProfile"
            android:exported="false" />
        <activity
            android:name=".Activity.WorkoutDetail"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.MyProfile"
            android:exported="false" />
        <activity
            android:name=".Activity.WorkoutList"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.TrainerDetail"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.MealDetail"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.ScanBody"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Success"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.Payment"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.Reservation"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.Home"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.SignUp"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.SignInOTP"
            android:exported="true"></activity>
        <activity
            android:name=".Activity.SignIn"
            android:exported="true">

        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.App.SplashScreen" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>